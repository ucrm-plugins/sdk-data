{
    "name": "ucrm-plugins/sdk-data",
    "description": "A data library for the UCRM Plugin SDK.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@spaethtech.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "name": "sdk", "type": "path", "url": "../*" }
    ],
    "require": {
        "php": "~7.3",

        "mvqn/data": "dev-master",
        "mvqn/dynamics": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "vlucas/phpdotenv": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "UCRM\\Data\\": [ "src/", "tests/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    }
}
